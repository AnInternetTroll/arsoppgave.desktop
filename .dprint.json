{
	"incremental": true,
	"useTabs": true,
	"lineWidth": 80,
	"typescript": {
		"binaryExpression.operatorPosition": "sameLine"
	},
	"includes": [
		"**/*.{ts,tsx,js,jsx,mjs,json,md}"
	],
	"excludes": [
		"**/node_modules",
		"**/*-lock.json"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.64.0.wasm",
		"https://plugins.dprint.dev/json-0.14.1.wasm",
		"https://plugins.dprint.dev/markdown-0.12.2.wasm"
	]
}
